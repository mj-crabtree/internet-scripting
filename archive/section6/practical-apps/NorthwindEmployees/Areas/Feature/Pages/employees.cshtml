@page
@using Matt.Shared
@model MyFeature.Pages.EmployeeModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
<div class="row">
    <h1 class="display-2">Employees</h1>
</div>
<div class="row">
    @foreach (var employee in Model.Employees)
    {
        <div class="col-sm-3">
            <partial name="_Employee" model="employee" />
        </div>
    }
</div>